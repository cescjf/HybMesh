$FUNC(
	$NAME grid_bnd_to_contour
	$DOC
		Builds contour out of grid boundary
	$ARG #GRID2D grid
	$ARG #BOOL simplify=#TRUE
	$RETURN #CONTOUR2D
$FUNC)

$FUNC(
	$NAME create_contour
	$DOC
		Creates contour from consequitives points
	$ARG #VECPOINT points
	$ARG #VECINT bnds
	$RETURN #CONTOUR2D
$FUNC)

$FUNC(
	$NAME create_spline_contour
	$DOC
		Creates singly connected contour as a parametric cubic spline.
	$ARG #VECPOINT points
	$ARG #VECINT bnds
	$ARG #INT nedges=100
	$RETURN #CONTOUR2D
$FUNC)

